{
	"name": "@ctx-core/git-tools",
	"version": "7.0.30",
	"description": "ctx-core git tools",
	"keywords": [
		"ctx-core",
		"git"
	],
	"homepage": "https://github.com/ctx-core/git-tools#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/git-tools/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/git-tools.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"ctx-core-git-hours": "bin/git-hours.sh",
		"git-hours": "bin/git-hours.sh",
		"timesheet": "bin/timesheet.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "tsc src/index.ts --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"git-csv-timesheet": "^0.1.5"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}
