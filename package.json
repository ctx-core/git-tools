{
	"name": "@ctx-core/git-tools",
	"version": "7.0.35",
	"description": "ctx-core git tools",
	"keywords": [
		"ctx-core",
		"git"
	],
	"homepage": "https://github.com/ctx-core/git-tools#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/git-tools/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/git-tools.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"ctx-core-git-hours": "bin/git-hours.sh",
		"git-hours": "bin/git-hours.sh",
		"timesheet": "bin/timesheet.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"git-csv-timesheet": "^0.1.5"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.125",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./lib/index.js"
}
