{
	"name": "@ctx-core/git-tools",
	"version": "6.0.4",
	"description": "ctx-core git tools",
	"keywords": [
		"ctx-core",
		"git"
	],
	"homepage": "https://github.com/ctx-core/functional#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/functional/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/functional/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"bin": {
		"ctx-core-git-hours": "bin/git-hours.sh",
		"git-hours": "bin/git-hours.sh",
		"timesheet": "bin/timesheet.js"
	},
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepare": "npm run build"
	},
	"dependencies": {
		"git-csv-timesheet": "^0.1.5"
	},
	"devDependencies": {
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}