{
	"name": "@ctx-core/git-tools",
	"version": "7.0.5",
	"description": "ctx-core git tools",
	"keywords": [
		"ctx-core",
		"git"
	],
	"homepage": "https://github.com/ctx-core/functional#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/functional/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/functional/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.js"
	},
	"bin": {
		"ctx-core-git-hours": "bin/git-hours.sh",
		"git-hours": "bin/git-hours.sh",
		"timesheet": "bin/timesheet.mjs"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"require = require('esm')(module/*, options*/)\nmodule.exports = require('./main.js')\nexport * from './index.js'\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"esm": "^3.2.25",
		"git-csv-timesheet": "^0.1.5"
	},
	"devDependencies": {
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
